{
  "config": {
    "target": "http://localhost",
    "socketio": {
      "query": "",
      "path": "/socketio/socket.io"
    },
    "phases": [
      {
        "duration": 30,
        "arrivalRate": 2
      }
    ]
  },
  "scenarios": [
    {
      "engine": "socketio",
      "flow": [
        {
          "emit": {
            "channel": "subscribe",
            "data": {
              "room": "room11",
              "command": "userJoin"
            },
            "namespace": "/webinar"
          }
        },
        {
          "think": 1
        },
        {
          "emit": {
            "channel": "publish",
            "data": {
              "room": "room11",
              "command": "userJoin",
              "data": {
                "name": "name-{{$randomString()}}"
              }
            },
            "namespace": "/webinar"
          }
        },
        {
          "think": 20
        }
      ]
    }
  ]
}
